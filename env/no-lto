_NOLTO_="-march=sandybridge -mno-avx -mno-aes -mno-rdrnd -mtune=sandybridge -O3 -pipe \
-falign-functions=32 -ffunction-sections -fdata-sections \
-fomit-frame-pointer -mllvm -polly -mllvm -polly-vectorizer=stripmine"
CFLAGS="${_NOLTO_}"
CXXFLAGS="${_NOLTO_}"
FCFLAGS="${_NOLTO_}"
FFLAGS="${_NOLTO_}"
